{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -H 0.0.0.0 -p 3000",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=2048\" next build",
    "build:low-memory": "cross-env NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=1536 --gc-interval=100\" next build",
    "build:monitor": "node scripts/monitor-build.js",
    "start": "cross-env NODE_OPTIONS=\"--max-old-space-size=512\" next start -p 3000",
    "lint": "next lint"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.19.3",
    "next": "15.3.2",
    "nodemailer": "^6.9.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.0.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
